<form action='/login' method='post'>
    <fieldset>  
      
        <legend>Log In</legend>
        <div>
            Username
        </div>
        <div>
			<input type='text' name='username' value='@locals.user.username' />
        </div>
        <div>
            Password
        </div>
        <div>
			<input type='text' name='password' value='@locals.user.password' />
        </div>
        <input type="submit" value="LogIn"/>
    </fieldset>
</form>
